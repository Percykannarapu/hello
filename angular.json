{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "applications",
  "projects": {
    "impower": {
      "root": "applications/impower",
      "sourceRoot": "applications/impower",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "webpack.conf.js"
            },
            "allowedCommonJsDependencies": [
              "xlsx",
              "moment"
            ],
            "outputPath": "dist/impower",
            "index": "applications/impower/index.html",
            "main": "applications/impower/main.ts",
            "tsConfig": "applications/impower/tsconfig.app.json",
            "polyfills": "applications/impower/polyfills.ts",
            "resourcesOutputPath": "assets/resources",
            "assets": [
              {
                "glob": "**/*",
                "input": "node_modules/@arcgis/core/assets",
                "output": "/assets/"
              },
              {
                "glob": "**/*",
                "input": "applications/impower/assets/val",
                "output": "/assets/"
              },
              {
                "glob": "*",
                "input": "applications/impower/assets/root",
                "output": "/"
              }
            ],
            "stylePreprocessorOptions": {
              "includePaths": []
            },
            "styles": [
              "node_modules/@arcgis/core/assets/esri/themes/light/main.css",
              {
                "input": "node_modules/font-awesome/css/font-awesome.css",
                "bundleName": "font-awesome"
              },
              {
                "input": "node_modules/primeflex/primeflex.scss",
                "bundleName": "primeflex"
              },
              {
                "input": "node_modules/primeng/resources/primeng.css",
                "bundleName": "primeng-core"
              },
              {
                "input": "node_modules/primeicons/primeicons.css",
                "bundleName": "primeng-icons"
              },
              "applications/impower/assets/theme/valassis/theme-light.scss",
              "applications/impower/assets/layout/css/layout-light.scss",
              "applications/impower/styles.scss"
            ],
            "scripts": [
              "node_modules/moment/min/moment.min.js"
            ],
            "optimization": false,
            "outputHashing": "none",
            "sourceMap": true,
            "namedChunks": true,
            "aot": false,
            "extractLicenses": false,
            "vendorChunk": true,
            "buildOptimizer": false,
            "webWorkerTsConfig": "applications/impower/tsconfig.worker.json"
          },
          "configurations": {
            "qa": {
              "polyfills": "applications/impower/polyfills.deploy.ts",
              "optimization": true,
              "outputHashing": "bundles",
              "sourceMap": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": false,
              "vendorChunk": false,
              "buildOptimizer": false,
              "fileReplacements": [
                {
                  "replace": "applications/impower/environments/environment.ts",
                  "with": "applications/impower/environments/environment.qa.ts"
                },
                {
                  "replace": "applications/impower/worker-shared/common/env/server-urls.ts",
                  "with": "applications/impower/worker-shared/common/env/server-urls.qa.ts"
                }
              ]
            },
            "production": {
              "polyfills": "applications/impower/polyfills.deploy.ts",
              "optimization": true,
              "outputHashing": "bundles",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": false,
              "vendorChunk": false,
              "buildOptimizer": false,
              "fileReplacements": [
                {
                  "replace": "applications/impower/environments/environment.ts",
                  "with": "applications/impower/environments/environment.prod.ts"
                },
                {
                  "replace": "applications/impower/worker-shared/common/env/server-urls.ts",
                  "with": "applications/impower/worker-shared/common/env/server-urls.prod.ts"
                }
              ]
            },
            "prod-win": {
              "polyfills": "applications/impower/polyfills.deploy.ts",
              "optimization": true,
              "outputHashing": "bundles",
              "sourceMap": false,
              "namedChunks": false,
              "aot": false,
              "extractLicenses": false,
              "vendorChunk": false,
              "buildOptimizer": false,
              "fileReplacements": [
                {
                  "replace": "applications/impower/environments/environment.ts",
                  "with": "applications/impower/environments/environment.prod.ts"
                },
                {
                  "replace": "applications/impower/worker-shared/common/env/server-urls.ts",
                  "with": "applications/impower/worker-shared/common/env/server-urls.prod.ts"
                }
              ]
            },
            "local-fuse": {
              "fileReplacements": [
                {
                  "replace": "applications/impower/environments/environment.ts",
                  "with": "applications/impower/environments/environment.local-fuse.ts"
                },
                {
                  "replace": "applications/impower/worker-shared/common/env/server-urls.ts",
                  "with": "applications/impower/worker-shared/common/env/server-urls.local.ts"
                }
              ]
            },
            "dev-server": {
              "polyfills": "applications/impower/polyfills.deploy.ts",
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "namedChunks": false,
              "aot": false,
              "extractLicenses": false,
              "vendorChunk": false,
              "buildOptimizer": false,
              "fileReplacements": [
                {
                  "replace": "applications/impower/environments/environment.ts",
                  "with": "applications/impower/environments/environment.dev.ts"
                }
              ]
            },
            "prod-as-dev": {
              "fileReplacements": [
                {
                  "replace": "applications/impower/environments/environment.ts",
                  "with": "applications/impower/environments/environment.prod-as-dev.ts"
                },
                {
                  "replace": "applications/impower/worker-shared/common/env/server-urls.ts",
                  "with": "applications/impower/worker-shared/common/env/server-urls.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "browserTarget": "impower:build"
          },
          "configurations": {
            "qa": {
              "browserTarget": "impower:build:qa"
            },
            "production": {
              "browserTarget": "impower:build:production"
            },
            "local-fuse": {
              "browserTarget": "impower:build:local-fuse"
            },
            "dev-server": {
              "browserTarget": "impower:build:dev-server"
            },
            "prod-win": {
              "browserTarget": "impower:build:prod-win"
            },
            "prod-as-dev": {
              "browserTarget": "impower:build:prod-as-dev"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "impower:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "applications/impower/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "applications/impower/polyfills.ts",
            "tsConfig": "applications/impower/tsconfig.spec.json",
            "scripts": [
              "node_modules/moment/moment.js"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "applications/impower/assets"
              ]
            },
            "styles": [
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/primeicons/primeicons.css",
              "applications/impower/assets/layout/css/layout-valassis-blue.scss",
              "applications/impower/assets/theme/theme-valassis-blue.scss",
              "applications/impower/styles.scss"
            ],
            "assets": [
              "applications/impower/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "applications/impower/tsconfig.app.json",
              "applications/impower/tsconfig.spec.json",
              "applications/impower/tsconfig.worker.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "cpq-maps": {
      "root": "applications/cpq-maps",
      "sourceRoot": "applications/cpq-maps/src",
      "projectType": "application",
      "prefix": "cpq",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "webpack.conf.js"
            },
            "outputPath": "dist/cpq-maps",
            "index": "applications/cpq-maps/src/index.html",
            "main": "applications/cpq-maps/src/main.ts",
            "polyfills": "applications/cpq-maps/src/polyfills.ts",
            "tsConfig": "applications/cpq-maps/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "node_modules/@arcgis/core/assets",
                "output": "/assets/"
              }
            ],
            "stylePreprocessorOptions": {
              "includePaths": []
            },
            "styles": [
              "node_modules/@arcgis/core/assets/esri/themes/light/main.css",
              "node_modules/font-awesome/css/font-awesome.css",
              "node_modules/primeng/resources/primeng.css",
              "node_modules/primeicons/primeicons.css",
              "applications/cpq-maps/src/assets/layout/css/layout-valassis-blue.scss",
              "applications/cpq-maps/src/assets/theme/theme-valassis-blue.scss",
              "applications/cpq-maps/src/styles.scss"
            ],
            "scripts": [],
            "optimization": false,
            "outputHashing": "none",
            "sourceMap": true,
            "extractCss": true,
            "namedChunks": true,
            "aot": false,
            "extractLicenses": false,
            "vendorChunk": true,
            "buildOptimizer": false
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "applications/cpq-maps/src/environments/environment.ts",
                  "with": "applications/cpq-maps/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "none",
              "sourceMap": false,
              "extractCss": false,
              "namedChunks": false,
              "aot": false,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": false
            },
            "qa": {
              "fileReplacements": [
                {
                  "replace": "applications/cpq-maps/src/environments/environment.ts",
                  "with": "applications/cpq-maps/src/environments/environment.qa.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "none",
              "sourceMap": false,
              "extractCss": false,
              "namedChunks": false,
              "aot": false,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": false
            }
          }
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "browserTarget": "cpq-maps:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "cpq-maps:build:production"
            },
            "qa": {
              "browserTarget": "cpq-maps:build:qa"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "cpq-maps:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "applications/cpq-maps/src/test.ts",
            "polyfills": "applications/cpq-maps/src/polyfills.ts",
            "tsConfig": "applications/cpq-maps/tsconfig.spec.json",
            "karmaConfig": "applications/cpq-maps/karma.conf.js",
            "stylePreprocessorOptions": {
              "includePaths": [
                "applications/cpq-maps/src/assets"
              ]
            },
            "styles": [
              "applications/cpq-maps/src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "applications/cpq-maps/src/favicon.ico",
              "applications/cpq-maps/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "applications/cpq-maps/tsconfig.app.json",
              "applications/cpq-maps/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "impower",
  "cli": {
    "warnings": {}
  },
  "schematics": {
    "@schematics/angular:class": {
      "skipTests": true
    },
    "@schematics/angular:component": {
      "prefix": "val",
      "style": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "val"
    }
  }
}
