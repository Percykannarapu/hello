{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "applications",
  "projects": {
    "impower": {
      "root": "applications/impower",
      "sourceRoot": "applications/impower",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "impower-dist",
            "index": "applications/impower/index.html",
            "main": "applications/impower/main.ts",
            "tsConfig": "applications/impower/tsconfig.app.json",
            "polyfills": "applications/impower/polyfills.ts",
            "assets": [
              "applications/impower/assets"
            ],
            "styles": [
              "applications/impower/styles.scss",
              "node_modules/font-awesome/css/font-awesome.css",
              "node_modules/primeng/resources/primeng.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.js",
              "node_modules/nanoscroller/bin/javascripts/jquery.nanoscroller.js",
              "applications/impower/assets/layout/js/ripple.js",
              "node_modules/moment/moment.js",
              "node_modules/chart.js/dist/Chart.js",
              "node_modules/fullcalendar/dist/fullcalendar.js"
            ]
          },
          "configurations": {
            "qa": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "applications/impower/environments/environment.ts",
                  "with": "applications/impower/environments/environment.qa.ts"
                }
              ]
            },
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "applications/impower/environments/environment.ts",
                  "with": "applications/impower/environments/environment.prod.ts"
                }
              ]
            },
            "prod-win": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "applications/impower/environments/environment.ts",
                  "with": "applications/impower/environments/environment.prod.ts"
                }
              ]
            },
            "local": {
              "fileReplacements": [
                {
                  "replace": "applications/impower/environments/environment.ts",
                  "with": "applications/impower/environments/environment.local.ts"
                }
              ]
            },
            "local-fuse": {
              "fileReplacements": [
                {
                  "replace": "applications/impower/environments/environment.ts",
                  "with": "applications/impower/environments/environment.local-fuse.ts"
                }
              ]
            },
            "dev-server": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "impower:build"
          },
          "configurations": {
            "qa": {
              "browserTarget": "impower:build:qa"
            },
            "production": {
              "browserTarget": "impower:build:production"
            },
            "local": {
              "browserTarget": "impower:build:local"
            },
            "local-fuse": {
              "browserTarget": "impower:build:local-fuse"
            },
            "dev-server": {
              "browserTarget": "impower:build:dev-server"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "impower:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "applications/impower/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "applications/impower/polyfills.ts",
            "tsConfig": "applications/impower/tsconfig.spec.json",
            "scripts": [
              "node_modules/jquery/dist/jquery.js",
              "node_modules/nanoscroller/bin/javascripts/jquery.nanoscroller.js",
              "applications/impower/assets/layout/js/ripple.js",
              "node_modules/moment/moment.js",
              "node_modules/chart.js/dist/Chart.js",
              "node_modules/fullcalendar/dist/fullcalendar.js"
            ],
            "styles": [
              "applications/impower/styles.scss",
              "node_modules/font-awesome/css/font-awesome.css",
              "node_modules/primeng/resources/primeng.css"
            ],
            "assets": [
              "applications/impower/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "applications/impower/tsconfig.app.json",
              "applications/impower/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "impower",
  "cli": {
    "warnings": {
      "typescriptMismatch": false
    }
  },
  "schematics": {
    "@schematics/angular:class": {
      "spec": false
    },
    "@schematics/angular:component": {
      "prefix": "val",
      "styleext": "css"
    },
    "@schematics/angular:directive": {
      "prefix": "val"
    }
  }
}
